class Pneu {
    public int ll;
    public int nb;

    public Pneu() {
    }

    public void setNb(int _nb) {
        this.nb = _nb;
    }
}

class Voiture {
    public int serie;
    public int titi;
    public Pneu pneu;
    public Voiture() {
    }
    public Pneu getPneu() {
        return this.pneu;
    }
}

public class Main {
	public static void main( String [] args)
	{
        Voiture v1 = new Voiture();
        Voiture v2 = new Voiture();
        Pneu p1 = new Pneu();
        Pneu p2 = new Pneu();
        v1.pneu = p1;
        v2.pneu = p2;
        p1.nb = 1;
        p2.nb = 2;
        v1.getPneu().setNb(10);
        print(v1.getPneu().nb);
        print(v2.getPneu().nb);
        p1 = v1.getPneu();
        print(p1.nb);
	}
}
