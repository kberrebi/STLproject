Interface Vehicule {
  void laver();
}

Classe Voiture implements Vehicule{
  int serie;
  public Voiture(int s){
  	serie=s;
  }
  public void laver(){
  	serie=0;
  }
  public void vendre(){
  	serie=10;
  }
}
